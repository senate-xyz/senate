{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build:db"
      ],
      "cache": false
    },
    "build:senate": {
      "dependsOn": [
        "^build:db"
      ],
      "cache": false
    },
    "build:shortie": {
      "dependsOn": [
        "^build:db"
      ],
      "cache": false
    },
    "build:detective": {
      "dependsOn": [
        "^db:generate"
      ],
      "cache": false
    },
    "build:refresher": {
      "dependsOn": [
        "^db:generate"
      ],
      "cache": false
    },
    "build:email-secretary": {
      "dependsOn": [
        "^db:generate"
      ],
      "cache": false
    },
    "build:discord-secretary": {
      "dependsOn": [
        "^db:generate"
      ],
      "cache": false
    },
    "build:telegram-secretary": {
      "dependsOn": [
        "^db:generate"
      ],
      "cache": false
    },
    "build:db": {
      "cache": false,
      "dependsOn": [
        "^db:generate"
      ]
    },
    "db:push": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "*": {
      "env": [
        "NODE_ENV",
        "EXEC_ENV",
        "PORT",
        "DATABASE_URL",
        "ALCHEMY_NODE_URL",
        "ARBITRUM_NODE_URL",
        "IPFS_GATEWAY_URL",
        "ETHERSCAN_API_KEY",
        "ARBISCAN_API_KEY",
        "NEXT_PUBLIC_WEB_URL",
        "NEXT_PUBLIC_ALCHEMY_KEY",
        "NEXTAUTH_SECRET",
        "OUTOFSERVICE",
        "DETECTIVE_URL",
        "POSTMARK_TOKEN",
        "VOTING_COUNTDOWN_TOKEN"
      ]
    },
    "start": {},
    "start:senate": {},
    "start:shortie": {},
    "dev": {
      "dependsOn": [
        "^db:generate",
        "^build:db"
      ]
    },
    "dev:senate": {
      "dependsOn": [
        "^db:generate",
        "^build:db"
      ]
    },
    "dev:bulletin": {
      "dependsOn": [
        "^db:generate",
        "^build:db"
      ]
    },
    "test:web": {
      "cache": false
    },
    "test:e2e": {
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "lint:fix": {
      "cache": false
    },
    "prettier": {
      "cache": false
    },
    "prettier:fix": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "update": {
      "cache": false
    }
  }
}
